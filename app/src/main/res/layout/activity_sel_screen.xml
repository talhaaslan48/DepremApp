<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:background="@drawable/orange1"
    android:layout_height="match_parent"
    tools:context=".SelFragment" >

    <TextView
        android:id="@+id/textView21"
        android:layout_width="299dp"
        android:layout_height="55dp"
        android:text=" Suyla sürüklenen enkazın yönünü kum torbalarıyla değiştirerek konutumuzdan uzak tutabiliriz."
        android:drawableLeft="@drawable/baseline_circle_24"
        android:drawablePadding="10dp"
        android:textColor="@color/black"
        android:textStyle="italic"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/textView9"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="@id/textView9"
        app:layout_constraintTop_toBottomOf="@id/textView9"
        app:layout_constraintVertical_bias="0.945"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="294dp"
        android:layout_height="43dp"
        android:text=" Pencere ve kapıları korumak için taşınabilir engeller yerleştirilebilir."
        android:drawableLeft="@drawable/baseline_circle_24"
        android:drawablePadding="10dp"
        android:textColor="@color/black"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@id/textView21"
        app:layout_constraintEnd_toEndOf="@id/textView20"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@id/textView20"
        app:layout_constraintTop_toBottomOf="@id/textView20"
        app:layout_constraintVertical_bias="0.827"/>

    <TextView
        android:id="@+id/textView18"
        android:layout_width="294dp"
        android:layout_height="43dp"
        android:text=" Su yatağı ve çukur bölgeler hemen terk edilmelidir."
        android:drawableLeft="@drawable/baseline_circle_24"
        android:drawablePadding="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/black"
        android:textStyle="italic"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/textView19"
        android:layout_width="297dp"
        android:layout_height="74dp"
        android:text=" Sel bölgesini hemen terk ederek yüksek ve güvenli bölgelere gitmeli ancak asla sudan karşıdan karşıya geçmeye çalışılmamalıdır, çünkü su aniden derinleşebilir."
        android:drawableLeft="@drawable/baseline_circle_24"
        android:drawablePadding="10dp"
        app:layout_constraintBottom_toTopOf="@id/textView20"
        android:textColor="@color/black"
        android:textStyle="italic"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="@id/textView18"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="@id/textView18"
        app:layout_constraintTop_toBottomOf="@id/textView18"
        app:layout_constraintVertical_bias="0.619"/>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="294dp"
        android:layout_height="43dp"
        android:text=" Konutu terk ederken elektrik ve su vanaları kapatılmalıdır."
        android:drawableLeft="@drawable/baseline_circle_24"
        android:drawablePadding="10dp"
        app:layout_constraintBottom_toTopOf="@id/textView9"
        android:textColor="@color/black"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="@id/textView19"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@id/textView19"
        app:layout_constraintTop_toBottomOf="@id/textView19"
        app:layout_constraintVertical_bias="0.72"/>

    <VideoView
        android:id="@+id/videoView2"
        android:layout_width="414dp"
        android:layout_height="322dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>